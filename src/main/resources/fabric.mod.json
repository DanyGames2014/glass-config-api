{
  "schemaVersion": 1,
  "id": "gcapi",
  "version": "${version}",

  "name": "Glass Config API",
  "description": "A shitty config GUI with an API to match.",
  "authors": [
    "calmilamsy"
  ],
  "contact": {
    "homepage": "https://glass-launcher.net/",
    "sources": "https://github.com/calmilamsy/Glass-Config-API"
  },

  "license": "CC0-1.0",
  "icon": "assets/gcapi/icon.png",

  "environment": "*",
  "entrypoints": {
    "gcapi" : [
      "net.glasslauncher.mods.api.gcapi.impl.example.ConfigClass"
    ]
  },
  "custom": {
    "modmenu:api": true
  },
  "mixins": [
    "gcapi.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3",
    "modmenu": "*"
  }
}
